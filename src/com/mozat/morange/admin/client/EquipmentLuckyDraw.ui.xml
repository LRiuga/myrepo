<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:datepicker="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		td {
			text-align: center;
		}
	</ui:style>
	<g:HTMLPanel>
		<h2>装备抽奖</h2>
		<table class="table table-bordered">
			<tr>
				<td>开放时间</td>
				<td>开启</td>
				<td>保存</td>
			</tr>
			<tr>
				<td><datepicker:DateBox ui:field="fromDate" /></td>
				<td><g:CheckBox ui:field="openCb"></g:CheckBox></td>
				<td><g:Button ui:field="saveBtn">保存</g:Button></td>
			</tr>
			
			
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 